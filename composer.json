{
    "name": "subhashladumor1/laravel-translate",
    "description": "üåê Professional multi-source translation package for Laravel 11+ with free APIs (LibreTranslate, Lingva, MyMemory), smart caching, batch translation, CLI tools, and analytics dashboard. Zero API costs!",
    "type": "library",
    "license": "MIT",
    "version":"1.0.1",
    "keywords": [
        "laravel",
        "translation",
        "translate",
        "multilingual",
        "i18n",
        "localization",
        "libretranslate",
        "lingva",
        "mymemory",
        "free-translation",
        "batch-translation",
        "offline-translation"
    ],
    "homepage": "https://github.com/subhashladumor1/laravel-translate",
    "authors": [
        {
            "name": "Subhash Ladumor",
            "homepage": "https://github.com/subhashladumor1",
            "role": "Laravel Package Developer"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/cache": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/session": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/view": "^9.0|^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit":  "^9.0|^10.0|^11.0|^12.0",
        "orchestra/testbench": "^9.0",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Subhashladumor1\\Translate\\": "src/"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Subhashladumor1\\Translate\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/pint"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Subhashladumor1\\Translate\\TranslateServiceProvider"
            ],
            "aliases": {
                "Translate": "Subhashladumor1\\Translate\\Facades\\Translate"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
